<structure>
	<type>lsystem</type>
	<!--The L-system axiom.-->
	<axiom>R ~ B</axiom>
	<!--The rewriting rules.-->
	<production>
		<left>B</left>
		<right>[ ~ ## T - B + + B - &amp; B ^ ^ B ]</right>
	</production>
	<production>
		<left>L</left>
		<right>[ angle=15 { - g + + g % - - g } ]</right>
	</production>
	<production>
		<left>R</left>
		<right>! @@ R</right>
	</production>
	<production>
		<left>T</left>
		<right>T g</right>
		<right>T</right>
	</production>
	<!--The drawing parameters.-->
	<parameter>
		<name>angle</name>
		<value>30</value>
	</parameter>
	<parameter>
		<name>color</name>
		<value>brown</value>
	</parameter>
	<parameter>
		<name>distance</name>
		<value>40</value>
	</parameter>
	<parameter>
		<name>polygonColor</name>
		<value>green</value>
	</parameter>
</structure>
